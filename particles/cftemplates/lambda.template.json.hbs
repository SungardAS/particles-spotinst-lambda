{{#layout}}
  {{parameter "m:lambda" "function/timeout" logicalId="Timeout" default="120"}}

  {{resource "m:lambda" "function/execution_role" logicalId="ExecutionRole"}}

  {{
    resource "lambda"
    logicalId="Lambda"
    timeout=(ref "Timeout")
    role=(partial "m:core" "fn_get_att" logicalId="ExecutionRole")
  }}

  {{set "m:lambda" "execution_role/all_outputs" roleLogicalId="ExecutionRole"}}
  {{set "m:lambda" "function/all_outputs" functionLogicalId="Lambda"}}

{{/layout}}
